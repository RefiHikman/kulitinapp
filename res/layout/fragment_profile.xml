<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="290dp"
                android:background="@drawable/roundtlbl"
                android:backgroundTint="#a280ec"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/Notif"
                    android:layout_width="13pt"
                    android:layout_height="13pt"
                    android:layout_marginRight="35dp"
                    app:tint="@color/white"
                    android:src="@drawable/belloutline"/>

                <ImageView
                    android:id="@+id/Cart"
                    android:layout_width="13pt"
                    android:layout_height="13pt"
                    android:layout_marginRight="35dp"
                    app:tint="@color/white"
                    android:src="@drawable/shoppingcart" />

                <ImageView
                    android:id="@+id/Setting"
                    android:layout_width="13pt"
                    android:layout_height="13pt"
                    app:tint="@color/white"
                    android:src="@drawable/settings" />

            </LinearLayout>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/fotoprofilbuled"
                android:layout_width="50pt"
                android:layout_height="50pt"
                android:layout_marginTop="45dp"
                android:background="@drawable/kyursetor"
                android:theme="@style/roundedImageView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/poppins_bold"
                android:text="Hello, ILesz"
                android:textColor="@color/white"
                android:textSize="9pt"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fotoprofilbuled" />

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="10dp"
                android:background="@color/white"
                android:elevation="10dp"
                android:padding="20dp"
                app:layout_constraintTop_toBottomOf="@+id/textView14"
                tools:layout_editor_absoluteX="20dp">

                <ImageView
                    android:id="@+id/wallet"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/wallet" />

                <TextView
                    android:id="@+id/kulitinwallet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="40dp"
                    android:layout_toRightOf="@+id/wallet"
                    android:fontFamily="@font/poppins"
                    android:text="Kulitin Wallet: Rp. 300.000"
                    android:textColor="@color/black"
                    android:textSize="13sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/kulitinwallet"
                    android:layout_marginLeft="60dp"
                    android:layout_toRightOf="@id/wallet"
                    android:fontFamily="@font/poppins_italic"
                    android:text="Click Here To Top Up"
                    android:textSize="12sp"
                    android:textStyle="italic" />
            </RelativeLayout>

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/poppins"
                android:text="Ongoing Rent"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/relativeLayout2" />

            <HorizontalScrollView
                android:id="@+id/scroll1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/textView15">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="#DDDDDD">

                    <RelativeLayout
                        android:layout_width="355dp"
                        android:layout_height="120dp"
                        android:layout_marginRight="10dp"
                        android:background="#EEEEEE">

                        <ImageView
                            android:id="@+id/ak1"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_marginLeft="20dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/item3"/>

                        <TextView
                            android:id="@+id/aktitle1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="40dp"
                            android:layout_toRightOf="@id/ak1"
                            android:text="AK47 - ASIIMOV"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:id="@+id/aksubtitle1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aktitle1"
                            android:layout_toRightOf="@id/ak1"
                            android:text="Duration Left : 13 Days"
                            android:textColor="@color/black"
                            android:textSize="11sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aksubtitle1"
                            android:layout_toRightOf="@id/ak1"
                            android:text="RP 29.999"
                            android:textColor="#FF5900"
                            android:textSize="15sp"
                            android:fontFamily="@font/poppins_bold"/>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="355dp"
                        android:layout_height="120dp"
                        android:layout_marginRight="10dp"
                        android:background="#EEEEEE">

                        <ImageView
                            android:id="@+id/ak2"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_marginLeft="20dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/item8"/>

                        <TextView
                            android:id="@+id/aktitle2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="40dp"
                            android:layout_toRightOf="@id/ak2"
                            android:text="AK47 - Golden Arabes"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:id="@+id/aksubtitle2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aktitle2"
                            android:layout_toRightOf="@id/ak2"
                            android:text="Duration Left : 29 Days"
                            android:textColor="@color/black"
                            android:textSize="11sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aksubtitle2"
                            android:layout_toRightOf="@id/ak2"
                            android:text="RP 31.999"
                            android:textColor="#FF5900"
                            android:textSize="15sp"
                            android:fontFamily="@font/poppins_bold"/>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="355dp"
                        android:layout_height="120dp"
                        android:background="#EEEEEE">

                        <ImageView
                            android:id="@+id/ak3"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_marginLeft="20dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/item7"/>

                        <TextView
                            android:id="@+id/aktitle3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="40dp"
                            android:layout_toRightOf="@id/ak3"
                            android:text="AK47 - Vulcan"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:id="@+id/aksubtitle3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aktitle3"
                            android:layout_toRightOf="@id/ak3"
                            android:text="Duration Left : 5 Days"
                            android:textColor="@color/black"
                            android:textSize="11sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aksubtitle3"
                            android:layout_toRightOf="@id/ak3"
                            android:text="RP 38.999"
                            android:textColor="#FF5900"
                            android:textSize="15sp"
                            android:fontFamily="@font/poppins_bold"/>
                    </RelativeLayout>

                </LinearLayout>
            </HorizontalScrollView>

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/poppins"
                android:text="Unpaid Rent"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/scroll1" />

            <HorizontalScrollView
                android:id="@+id/scroll2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="20dp"
                app:layout_constraintTop_toBottomOf="@id/textView16">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="#DDDDDD">

                    <RelativeLayout
                        android:layout_width="355dp"
                        android:layout_height="120dp"
                        android:layout_marginRight="10dp"
                        android:background="#EEEEEE">

                        <ImageView
                            android:id="@+id/ak1"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_marginLeft="20dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/item3"/>

                        <TextView
                            android:id="@+id/aktitle1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="40dp"
                            android:layout_toRightOf="@id/ak1"
                            android:text="AK47 - ASIIMOV"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:id="@+id/aksubtitle1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aktitle1"
                            android:layout_toRightOf="@id/ak1"
                            android:text="Duration Left : 13 Days"
                            android:textColor="@color/black"
                            android:textSize="11sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aksubtitle1"
                            android:layout_toRightOf="@id/ak1"
                            android:text="RP 29.999"
                            android:textColor="#FF5900"
                            android:textSize="15sp"
                            android:fontFamily="@font/poppins_bold"/>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="355dp"
                        android:layout_height="120dp"
                        android:layout_marginRight="10dp"
                        android:background="#EEEEEE">

                        <ImageView
                            android:id="@+id/ak2"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_marginLeft="20dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/item8"/>

                        <TextView
                            android:id="@+id/aktitle2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="40dp"
                            android:layout_toRightOf="@id/ak2"
                            android:text="AK47 - Golden Arabes"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:id="@+id/aksubtitle2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aktitle2"
                            android:layout_toRightOf="@id/ak2"
                            android:text="Duration Left : 29 Days"
                            android:textColor="@color/black"
                            android:textSize="11sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aksubtitle2"
                            android:layout_toRightOf="@id/ak2"
                            android:text="RP 31.999"
                            android:textColor="#FF5900"
                            android:textSize="15sp"
                            android:fontFamily="@font/poppins_bold"/>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="355dp"
                        android:layout_height="120dp"
                        android:background="#EEEEEE">

                        <ImageView
                            android:id="@+id/ak3"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_marginLeft="20dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/item7"/>

                        <TextView
                            android:id="@+id/aktitle3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="40dp"
                            android:layout_toRightOf="@id/ak3"
                            android:text="AK47 - Vulcan"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:id="@+id/aksubtitle3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aktitle3"
                            android:layout_toRightOf="@id/ak3"
                            android:text="Duration Left : 5 Days"
                            android:textColor="@color/black"
                            android:textSize="11sp"
                            android:fontFamily="@font/poppins"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="40dp"
                            android:layout_below="@id/aksubtitle3"
                            android:layout_toRightOf="@id/ak3"
                            android:text="RP 38.999"
                            android:textColor="#FF5900"
                            android:textSize="15sp"
                            android:fontFamily="@font/poppins_bold"/>
                    </RelativeLayout>

                </LinearLayout>
            </HorizontalScrollView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>



</androidx.constraintlayout.widget.ConstraintLayout>